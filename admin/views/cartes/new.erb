<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to pat(:list), url(:cartes, :index) %></li>
      <li class="active"><%= link_to pat(:new), url(:cartes, :new) %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"><%= pat(:new) %> <% mt(:carte) %></h2>
    <div class="inner">
      <% form_for :carte, url(:cartes, :create), :class => :form, :multipart => true do |f| %>
        <%= partial "cartes/form", :locals => { :f => f } %>
      <% end %>
    </div>
  </div>
</div>
<% content_for :sidebar, partial("base/sidebar") %>
